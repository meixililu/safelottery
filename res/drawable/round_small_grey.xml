<?xml version="1.0" encoding="utf-8"?>
<!--空心圆角矩形  -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
 
    <corners android:radius="5dp" /><!-- 圆角半径 -->
 
    <size
        android:height="5dp"
        android:width="5dp" /><!--矩形的宽高  -->
 
    <solid
        android:color="@color/none_thin" /><!-- 矩形的内部颜色，这里设置成白色，即空心 -->
 
</shape>